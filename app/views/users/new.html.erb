<%= form_with(url: "/users", method: "post") do  %>
    <%= label_tag(:email, "Email:") %>
    <%= email_field(:user, :email )%>
    <%= label_tag(:username, "Username:") %>
    <%= text_field(:user, :username) %>
    <%= label_tag(:password, "Password:") %>
    <%= password_field(:user, :password) %>
    <%= submit_tag("Sign up") %>
<% end %>